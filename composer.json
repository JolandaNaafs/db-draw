{
  "name": "jawira/db-draw",
  "type": "library",
  "description": "\ud83d\udcf8 Create an ER diagram from your existing DB",
  "keywords": [
    "database",
    "db",
    "dbal",
    "diagram",
    "doctrine"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jawira Portugal",
      "email": "dev@tugal.be"
    }
  ],
  "require": {
    "php": ">=7.3",
    "doctrine/dbal": "^2",
    "jawira/the-lost-functions": "^1.1"
  },
  "require-dev": {
    "ext-dom": "*",
    "ext-mbstring": "*",
    "ext-xml": "*",
    "doctrine/orm": "^2.8",
    "ergebnis/composer-normalize": "^2.13",
    "friendsofphp/php-cs-fixer": "^3.0",
    "jawira/plantuml-client": "^1.0",
    "jawira/skeleton": "^2.0",
    "phpstan/phpstan": "^0.12.82",
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "preferred-install": "dist"
  },
  "autoload": {
    "psr-4": {
      "Jawira\\DbDraw\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jawira\\DbDraw\\Tests\\": "tests"
    }
  }
}
