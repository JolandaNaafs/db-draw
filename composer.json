{
  "name": "jawira/db-visualizer",
  "type": "library",
  "description": "Create a diagram from your DB",
  "license": "MIT",
  "authors": [
    {
      "name": "jawira",
      "email": "dev@tugal.be"
    }
  ],
  "require": {
    "php": ">=7.3",
    "jawira/plantuml-client": "^0.1.0",
    "jawira/the-lost-functions": "^1.1",
    "doctrine/dbal": "^2"
  },
  "require-dev": {
    "ext-dom": "*",
    "ext-mbstring": "*",
    "ext-xml": "*",
    "ergebnis/composer-normalize": "^2.13",
    "phpstan/phpstan": "^0.12.82",
    "phpunit/phpunit": "^9.5",
    "doctrine/orm": "^2.8"
  },
  "autoload": {
    "psr-4": {
      "Jawira\\DbVisualizer\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jawira\\DbVisualizer\\Tests\\": "tests"
    }
  }
}
