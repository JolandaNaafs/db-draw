{
  "name": "jawira/db-visualizer",
  "type": "library",
  "description": "Create a diagram from your DB",
  "license": "MIT",
  "authors": [
    {
      "name": "jawira",
      "email": "dev@tugal.be"
    }
  ],
  "require": {
    "php": ">=7.3",
    "doctrine/dbal": "^3.0",
    "jawira/plantuml-client": "^0.1.0"
  },
  "require-dev": {
    "ext-dom": "20031129",
    "ext-mbstring": "^7.3",
    "ext-xml": "^7.3",
    "ergebnis/composer-normalize": "^2.13",
    "phpstan/phpstan": "^0.12.82",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Jawira\\DbVisualizer\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jawira\\DbVisualizer\\Tests\\Type\\": "tests/Type"
    }
  }
}
